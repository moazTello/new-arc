{
	"name": "new-arc",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build && cp -r public .next/standalone/ && cp -r .next/static .next/standalone/.next/",
		"start": "next start",
		"db:prepare": "prisma generate && prisma migrate deploy",
		"db:backup": "node ./scripts/backup.mjs",
		"db:restore": "node --env-file=.env ./scripts/restore-backup.mjs",
		"create:user": "node --env-file=.env ./scripts/create-user.mjs",
		"next:lint": "next lint",
		"lint": "biome check . --write",
		"format": "biome format . --write"
	},
	"prisma": {
		"schema": "db/schema.prisma"
	},
	"dependencies": {
		"@ant-design/nextjs-registry": "^1.0.2",
		"@ant-design/v5-patch-for-react-19": "^1.0.3",
		"@clack/prompts": "^0.11.0",
		"@prisma/client": "^6.8.2",
		"@tanstack/react-query": "^5.77.0",
		"antd": "^5.25.2",
		"bcrypt": "^6.0.0",
		"better-sqlite3": "^11.10.0",
		"dayjs": "^1.11.13",
		"json-as-xlsx": "^2.5.6",
		"lucide-react": "^0.511.0",
		"next": "15.3.2",
		"next-auth": "5.0.0-beta.28",
		"nuqs": "^2.4.3",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"zod": "^3.25.28"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@tailwindcss/postcss": "^4",
		"@types/bcrypt": "^5.0.2",
		"@types/better-sqlite3": "^7.6.13",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"prisma": "^6.8.2",
		"tailwindcss": "^4",
		"typescript": "^5"
	}
}
